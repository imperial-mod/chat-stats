{
    "compilerOptions": {
        "target": "es5",
    	"module": "commonjs",
    	"declaration": false,
    	"outDir": "./dist",
    	"strict": true,
		"noImplicitAny": true,
		"alwaysStrict": true,
		"strictPropertyInitialization": false,
		"downlevelIteration": true,
		"esModuleInterop": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ],
	"include": [
		"src"
	]
}